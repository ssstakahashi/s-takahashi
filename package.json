{
  "name": "s-takahashi",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "npx astro",
    "wrangler": "npx wrangler",
    "pages:dev": "wrangler pages dev --compatibility-date=2024-01-17 -- astro dev",
    "pages:deploy": "astro build && wrangler pages deploy ./dist",
    "format": "prettier --write 'src/**/*.{js,astro,md,ts}'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/cloudflare": "^11.0.4",
    "@astrojs/markdown-remark": "^5.2.0",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/noto-sans-jp": "^5.0.20",
    "@fontsource/noto-sans-jp": "^5.0.20",
    "@types/canvas-confetti": "^1.6.4",
    "astro": "^4.14.2",
    "autoprefixer": "^10.4.20",
    "canvas-confetti": "^1.9.3",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-astro"
    ]
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "wrangler": "^3.72.0"
  }
}
